{
  "name": "bytic/form-builder",
  "description": "Form builder module for bytic library",
  "license": "MIT",
  "type": "library",
  "keywords": [],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/ByTIC/form-builder",
  "require": {
    "bytic/form": "^1.0|^2.0",
    "bytic/models-smart-properties": "^1.0|^2.0",
    "bytic/orm-behaviors": "^1.0|^2.0",
    "bytic/cache": "^1.0|^2.0",
    "bytic/orm-filters": "^1.0|^2.0",
    "bytic/package-base": "^1.0|^2.0",
    "bytic/translation": "^1.0|^2.0",
    "bytic/actions": "^1.0|^2.0|dev-main",
    "giggsey/libphonenumber-for-php": "^8.13"
  },
  "require-dev": {
    "bytic/common": "^1.0|^2.0",
    "bytic/google-recaptcha": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\FormBuilder\\": "src/",
      "ByTIC\\FormBuilder\\Application\\": "src/application/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\FormBuilder\\Tests\\": "tests/src",
      "ByTIC\\FormBuilder\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "bytic/phpqatools": true,
      "php-http/discovery": true
    }
  }
}
